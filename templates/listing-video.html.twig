{% extends 'partials/base.html.twig' %}

{% block content %}
    {{ page.content }}
    <div class="grid">
    {% for v in config.plugins.cloudinary.list %}
    <a href="{{ v.url }}"><img src="{{ v.url_img }}" alt="{{ t("PLUGIN_CLOUDINARY.ALT_PREFIX") }} {{ v.title }}">{{ v.title }}</a>
    {% endfor %}
    </div>
{% endblock %}
